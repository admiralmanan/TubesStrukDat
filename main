#include "tGraph.h"
#include <iostream>
using namespace std;

int main() {
    Graph G;
    initGraph(G);

    addVertex(G, "Gedung_A");
    addVertex(G, "Gedung_B");
    addVertex(G, "Gedung_C");
    addVertex(G, "Gedung_D");
    addVertex(G, "Gedung_E");
    addVertex(G, "Gedung_F");
    addVertex(G, "Gedung_G");
    addVertex(G, "Gedung_H");
    addVertex(G, "Gedung_I");
    addVertex(G, "Gedung_J");

    addEdge(G, "Gedung_A", "Gedung_B", 1);
    addEdge(G, "Gedung_B", "Gedung_C", 1);
    addEdge(G, "Gedung_C", "Gedung_D", 1);
    addEdge(G, "Gedung_D", "Gedung_E", 5);
    addEdge(G, "Gedung_D", "Gedung_F", 3);
    addEdge(G, "Gedung_E", "Gedung_F", 3);
    addEdge(G, "Gedung_F", "Gedung_G", 5);
    addEdge(G, "Gedung_F", "Gedung_H", 4);
    addEdge(G, "Gedung_G", "Gedung_H", 3);
    addEdge(G, "Gedung_G", "Gedung_J", 4);
    addEdge(G, "Gedung_H", "Gedung_I", 3);
    addEdge(G, "Gedung_I", "Gedung_J", 3);

    tampilkanMenu(G);
    return 0;
}
